<%- include('./partials/header.ejs') %>
<div class="w-full min-h-screen bg-zinc-700 flex px-10 text-white">
    <div class="cards flex flex-wrap gap-10 mt-8">
        <% user.posts.forEach(function(elem){%>
            <div class="card w-40">
                <div class="w-full bg-sky-200 rounded-lg">
                    <img src="/images/uploads/<%= elem.image %>" class="object-fit rounded-lg" alt="">
                </div>
                <h5 class="mt-2 text-sm"><%= elem.title %></h5>
            </div>
        <% }) %>
    </div>
</div>
<%- include('./partials/footer.ejs') %>
