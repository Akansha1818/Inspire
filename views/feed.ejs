<%- include('./partials/header.ejs') %>
<div class="w-full min-h-screen bg-zinc-700 flex px-10 text-white">
    <div class="cards flex flex-wrap gap-10 mt-8">
        <% posts.forEach(function(elem){%>
            <div class="card w-40">
                <div class="rounded-lg">
                    <img src="/images/uploads/<%= elem.image %>" class="w-full h-full object-cover rounded-lg" alt="">
                </div>
                <h5 class="mt-2 text-sm"><%= elem.title %></h5>
                <h6><%= elem.user.username %></h6>
            </div>
        <% }) %>
    </div>
</div>
<%- include('./partials/footer.ejs') %>
